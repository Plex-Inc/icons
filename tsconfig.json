{
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "scripts",
  ]
}